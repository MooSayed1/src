{
   "background": {
      "scripts": [ "background.js" ]
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "content_security_policy": "script-src 'self'",
   "description": "Quran Tab is an Islamic new tab page that will keep you inspired by a verse from the Quran every time you open a new tab.",
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Quran Tab",
   "permissions": [ "notifications", "alarms", "https://api.alquran.cloud/*", "https://*.islamic.network/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.6.4"
}
